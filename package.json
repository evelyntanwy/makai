  "main": "server/index.js",
  "author": "EDA",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config ./client/webpack.config.js",
    "dev:client": "npm run build -- --watch",
    "dev:server": "nodemon server",
    "dev": "run-p dev:client dev:server",
    "knex": "knex --knexfile server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "eslint . --fix",
    "start": "node server",
    "test": "jest --watchAll --verbose"
  },
  "keywords": [
    "fullstack"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "knex": "^0.21.1",
    "react-bootstrap": "^2.1.2",
    "sqlite3": "^4.2.0",
    "node-fetch": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "superagent": "^5.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "eslint": "7.9.0",
    "eslint-config-eda": "0.1.4",
    "jest": "^27.4.7",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^6.2.1",
    "superagent": "^5.3.1",
    "supertest": "^6.2.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "engines": {
    "node": "14.x"
  }
}
